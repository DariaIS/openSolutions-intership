{
  "name": "boilerplate",
  "version": "0.0.0",
  "private": true,
  "description": "for starting everything",
  "main": "src/index.tsx",
  "license": "Proprietary software, no public license. SEE LICENSE IN LICENSE FILE",
  "author": "Alex Maruga <avataka9@gmail.com>",
  "engines": {
    "node": ">=16.13.2",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "check-type": "tsc --noEmit",
    "lint": "eslint \"src/**/*.+(ts|tsx|js|jsx)\"",
    "stylelint": "stylelint src",
    "prettier": "prettier \"src/**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run stylelint && npm run lint && npm run prettier -- --list-different",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "prepare": "husky install",
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@microsoft/signalr": "^6.0.3",
